# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'PodExample2' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for PodExample2
  pod 'Alamofire'
  pod 'SwiftyJSON'

  target 'PodExample2Tests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'PodExample2UITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
